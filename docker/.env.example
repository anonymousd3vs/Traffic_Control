# Traffic Control System - Docker Environment Variables
# Copy this file to .env and update values as needed

# Backend Configuration
BACKEND_HOST=0.0.0.0
BACKEND_PORT=8765
DETECTION_MODE=video
PYTHON_ENV=production

# Frontend Configuration
VITE_API_URL=http://localhost:8765
FRONTEND_PORT=3000

# Video Configuration
VIDEO_PATH=/app/videos
VIDEO_PLAYBACK_FPS=25

# Model Configuration
MODEL_PATH=/app/models
ONNX_MODEL=indian_ambulance_yolov11n_best.onnx
PYTORCH_MODEL=indian_ambulance_yolov11n_best.pt
USE_ONNX=true

# Lane Configuration
LANE_CONFIG_PATH=/app/config/lane_config.json

# Detection Settings
CONFIDENCE_THRESHOLD=0.5
IOU_THRESHOLD=0.45
VEHICLE_ONLY=true

# Logging
LOG_LEVEL=INFO
LOG_PATH=/app/logs

# System
WORKERS=4
MAX_RETRIES=3
TIMEOUT=30
